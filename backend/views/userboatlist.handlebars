<h2 class="page-header">Boat List</h2>
<table>
    <tr>
        <th>Boat Name</th>
        <th>Boat Length</th>
        <th>Boat Year</th>
        <th>Boat Capacity</th>
        <th>Boat Pic</th>
    </tr>
    {{#each boats}}
        <tr>
            <td headers="name">{{this.boatname}}</td>
            <td headers="name">{{this.boatlengthinfeet}}</td>
            <td headers="name">{{this.boatyear}}</td>
            <td headers="name">{{this.boatcapacityinpeople}}</td>
            <td headers="name"><img src="{{this.boatpictureurl}}" width="100px"></td>
        </tr>
    {{/each}}
</table>